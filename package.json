{
  "name": "aguka",
  "version": "1.0.0",
  "description": "aguka backend project",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Solvit-Africa-Training-Center/aguka_backend.git"
  },
  "author": "jbniyikiza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Solvit-Africa-Training-Center/aguka_backend/issues"
  },
  "homepage": "https://github.com/Solvit-Africa-Training-Center/aguka_backend#readme",
  "scripts": {
    "test": "npm run undo-migration  && npm run migration && npm run seed && jest --detectOpenHandles",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test:coverage": "jest --coverage ",
    "coverage:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
    "coverage:qlty": "npm run test:coverage && qlty report --coverage-file coverage/lcov.info",
    "coverage:report": "qlty report --coverage-file coverage/lcov.info",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "migration:create": "sequelize-cli migration:generate --name",
    "migration:run": "npx sequelize-cli db:migrate",
    "seed:create": "sequelize-cli seed:generate --name",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^1.41.3",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "i18next": "^25.4.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.8.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.1",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/redis": "^4.0.10",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "babel-jest": "^30.0.5",
    "babel-register": "^6.26.0",
    "coveralls": "^3.1.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "jiti": "^2.5.1",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
